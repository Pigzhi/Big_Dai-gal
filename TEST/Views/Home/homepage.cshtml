                               $(function() {
                                // 取得訂單列表
                                $.ajax({
                                    url: '/api/orders/list',
                                    type: 'GET',
                                    success: function(data) {
                                        $.each(data, function(index, item) {
                                           console.log(data);
                                            // 處理圖片路徑
                                            var imgPath = item.img;
                                            if (!imgPath || imgPath === "") {
                                                imgPath = '/img/iphone13.jpg'; // 現在沒圖片預設圖片
                                            } else if (imgPath.startsWith('/images/')) {
                                                imgPath = imgPath.replace('/images/', '/img/');
                                            }

                                            // 每個訂單 
                                            var html = `
                            <div class="col-lg-4 col-md-6 wow fadeInUp order-item" data-id="${item.orderId}">
                                <div class="property-item rounded overflow-hidden">
                                    <div class="position-relative overflow-hidden">
                                        <img class="img-fluid w-100 h-100 object-fit-cover" src="${imgPath}" alt="">
                                        <div class="bg-primary rounded text-white position-absolute start-0 top-0 m-4 py-1 px-3">${item.productType}</div>
                                        <div class="bg-white rounded-top text-primary position-absolute start-0 bottom-0 mx-4 pt-1 px-3">${item.category}</div>
                                    </div>
                                    <div class="p-4 pb-0">
                                        <h5 class="text-primary mb-3">${item.price}</h5>
                                        <a class="d-block h5 mb-2" href="javascript:void(0)">${item.title}</a>
                                        <p><i class="fa fa-map-marker-alt text-primary me-2"></i>${item.location ?? '未提供'}</p>
                                    </div>
                                    <div class="d-flex border-top">
                                        <small class="flex-fill text-center border-end py-2"><i class="fa-solid fa-clock text-primary me-2"></i>剩餘天數 : </small>
                                        <small class="flex-fill text-center py-2"><i class="fa fa-address-card text-primary me-2"></i>${item.createdAt}</small>
                                    </div>
                                </div>
                            </div>`;
                                            $('#orderContainer').append(html);
                                        });
                                    }
                                });

                                // 點擊 order-item 打開 modal
                                $(document).on('click', '.order-item', function(e) {
                                                            e.stopPropagation(); // 阻止冒泡
                            var id = $(this).closest('.order-item').data('id');
                            console.log('點擊的訂單 ID:', id);
                            if (!id) return;
                                    $.ajax({
                                        url: '/api/orders/' + id,  // 注意路由要跟 Controller 一致
                                        type: 'GET',
                                        success: function(order) {
                                            // 填入 modal
                                            var modalHtml = `
                              <img src="${order.img}">
                            <div class="info">
                                <p>訂單 ID：${order.orderId}</p>
                                <p>商品名稱：${order.title}</p>
                                <p>價格：${order.price}</p>
                                <p>分類：${order.category}</p>
                                <p>地點：${order.location ?? '未提供'}</p>
                            </div>
                            `;
                            $('#orderModalBody').html(modalHtml);

                            // 顯示你自己的 modal
                            $('#orderOverlay').addClass('show');
                                        },
                                        error: function() {
                                            alert('找不到這筆訂單');
                                        }
                                    });
                                });
                            });

                              
                        </script>

                        

